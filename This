          
class A{
    public A(){
        super();
        System.out.println("in A");
    }
    public A(int n){
        super();
        System.out.println("in A int ");
    }
}

class B extends A{
    public B(){
        super();
        System.out.println("In B");
    }

    public B(int n){
        //super();
        this();
        System.out.println("in B int ");
    }
}
public class This {
    public static void main(String[] args) {
       new B(5); 
    }
}
